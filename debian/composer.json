{
  "name": "deb/abraflexi-bricks",
  "description": "php-flexibee extending classes and widgets (debianized)",
  "homepage": "https://github.com/VitexSoftware/AbraFlexi-Bricks",
  "license": "GPL-2.0+",
  "version": "1.2",
  "type": "library",
  "authors": [
    {
      "name": "Vítězslav Dvořák",
      "email": "vitex@arachne.cz"
    }
  ],
  "require": {
    "deb/abraflexi": "*",
    "deb/ease-bricks": "*"
  },
  "autoload": {
    "psr-4": {
      "AbraFlexi\\Bricks\\": "./Bricks",
      "AbraFlexi\\Bricks\\ConvertRules\\": "./Bricks/ConvertRules",
      "AbraFlexi\\ui\\": "./ui"
    }
  },
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "path",
      "url": "/usr/share/php/EaseCore"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseHtml"
    },
    {
      "type": "path",
      "url": "/usr/share/php/EaseBricks"
    },
    {
      "type": "path",
      "url": "/usr/share/php/AbraFlexi"
    }
  ],
  "address": "0.32"
}
